{
    "name": "hexlet/code",
    "description": "Generate diff of config files",
    "type": "project",
    "keywords": [
        "diff",
        "tool",
        "hexlet"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "bsv",
            "email": "bsv.phone@ya.ru"
        }
    ],
    "require": {
        "php": "^7.4.1",
        "docopt/docopt": "^1.0.4",
        "symfony/string": "^5.1",
        "symfony/yaml": "^5.0",
        "tightenco/collect": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1.3",
        "squizlabs/php_codesniffer": "^3.5.5",
        "phpstan/phpstan": "^0.12.38",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpstan/extension-installer": "^1.0",
        "psy/psysh": "@stable",
        "symfony/var-dumper": "^4.0"
    },
    "autoload": {
        "files": [
            "src/cliHandler.php",
            "src/actions.php",
            "src/parsers.php",
            "src/renderers.php",
            "src/diffTree.php"
        ]
    },
    "bin": [
        "bin/gendiff"
    ],
    "scripts": {
        "phpcs": "vendor/bin/phpcs",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
